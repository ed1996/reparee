                          <select class="form-control border rounded-pill">
                            <option>Default select</option>
                          </select>
                          <button type="submit" class="btn btn-pills btn-primary">Get Started</button>
                        </div>
                      </form>
                    </div>-->

          <div class="title-heading text-center mt-4 mt-md-5 pt-md-5">
              <%= form_tag search_path, method: :get, class: "rounded p-4 mt-4 bg-white" do %>
                <div class="row text-left">
                  <div class="col-lg-5 col-md-4">
                    <div class="form-group mb-0">
                      <%= text_field_tag :search, params[:search], placeholder: "Dans quelle ville ?", class:"form-control" %>
                    </div>
                  </div><!--end col-->

                  <div class="col-lg-7 col-md-8">
                    <div class="row align-items-center">
                      <div class="col-md-8 mt-8 mt-sm-0">
                        <div class="form-group mb-0">
                          <%= select :artisan_type, params[:artisan_type], [["Plombier","Plombier"],["Serrurier","Serrurier"]], {}, {placeholder: "Quelle type d'artisan ?", class:"form-control custom-select"} %>
                        </div>
                      </div><!--end col-->

                      <div class="col-md-4 mt-4 mt-sm-0">
                        <%= submit_tag "Rechercher", class:"searchbtn btn btn-primary btn-block p" %>
                      </div><!--end col-->
                    </div><!--end row-->
                  </div> <!---end col-->
                </div><!--end row-->
              <% end %>
          </div>
        </div>
        <div class="row justify-content-center">
          <div class="col-lg-10 text-center">
            <div class="home-dashboard">
              <%= image_tag "laptop.png", :class => "img-fluid", :alt => "dashboard" %>
            </div>
          </div>
        </div>
      </div><!--end col-->
    </div><!--end row-->
  </div><!--end container-->
</section><!--end section-->
<!-- End Home -->

